package json;

import map.map;
import stmt.Program;

public class main {
    public static void main(String[] args) {
        lexer L = new lexer("""
        {
            \"Hello\":\"World\",
            \"a\":{
                \"b\":\"e\"
            }
        }
        """);
        Token[] Tokens = L.Tokenize();
        parser P = new parser(Tokens);
        stmt.Program B = P.Parse();
        for (int i = 0; i < Tokens.length; i++) {
            System.out.println(Tokens[i].Type);  
        }
        mapper M = new mapper();
        map F = (map) M.Eval(B);
        F.dump();
        map F2 = (map) F.find("body");
        F2.dump();
        map F3 = (map) F2.find("Hello");
        System.out.println(F3);
        // F3.dump();
        // map F3 = (map) F2.find("a");
        // F3.dump();
        // System.out.println(F.find("H"));
    }

}